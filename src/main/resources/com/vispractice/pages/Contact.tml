<html t:type="layout" title="Contact com.vispractice"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
<t:form>
	<t:select t:id="league" t:value="leagueId" t:model="sm" t:zone="gzone" 
		style="width: 20%;"/>
	<hr />
	<t:zone t:id="gzone">
		<div>
			<t:grid source="gds" t:exclude="id" t:rowsPerPage="25"
				t:add="league,vs" t:row="match" class="table table-bordered">
				<p:vsCell>
			${match.home.name} vs ${match.guest.name}
		</p:vsCell>
				<p:kickoffCell>
					<t:output value="match.kickoff" format="sdf" />
				</p:kickoffCell>
				<p:leagueCell>
			${match.league.name}
		</p:leagueCell>
			</t:grid>
		</div>
	</t:zone>
</t:form>

</html>
